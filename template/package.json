{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    "build": "set PRODUCTION=1 && npm run build:production",
    "test": "set TEST=1 && npm run build:production",
    "dev": "node npm-scripts/clean-dev.js && set DEV=1 && webpack --config build/webpack/development.js --progress --watch",
    "build:production": "node npm-scripts/clean-build.js && webpack --config build/webpack/production.js --progress --bail",
    "dll": "node npm-scripts/clean-dll.js && webpack --config build/webpack/dll.js --progress --colors"{{#lint}},
    "lint": "eslint --ext .js,.vue src{{#unit}} test/unit/specs{{/unit}}{{#e2e}} test/e2e/specs{{/e2e}}"{{/lint}}
  },
  "dependencies": {
    "fastclick": "^1.0.6",
    "lib-flexible": "^0.3.2",
    "vue": "^2.1.10"{{#router}},
    "vue-router": "^2.2.0"{{/router}}
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.1",
    {{#lint}}
    "babel-eslint": "^7.1.1",
    {{/lint}}
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.1.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "browser-sync": "^2.17.5",
    "browser-sync-ui": "^0.6.1",
    "browser-sync-webpack-plugin": "^1.1.3",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.26.1",
    {{#lint}}
    "eslint": "^3.14.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    {{#if_eq lintConfig "standard"}}
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    {{/if_eq}}
    {{#if_eq lintConfig "airbnb"}}
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    {{/if_eq}}
    {{/lint}}

    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "happypack": "^3.0.1",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "os": "^0.1.1",
    "postcss-loader": "^0.13.0",
    "postcss-pxtorem": "^4.0.0",
    "precss": "^1.4.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.3.0",
    "vue-style-loader": "^2.0.0",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^1.13.2",
